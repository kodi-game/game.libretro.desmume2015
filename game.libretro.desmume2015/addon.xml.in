<?xml version="1.0" encoding="UTF-8"?>
<addon id="game.libretro.desmume2015"
		name="Nintendo - DS (DeSmuME 2015)"
		version="0.0.1.7"
		provider-name="YopYop156, Zeromus">
	<requires>
		<import addon="game.libretro" version="1.0.0"/>
	</requires>
	<extension point="kodi.gameclient"
			library_@PLATFORM@="@LIBRARY_FILENAME@">
		<platforms></platforms>
		<extensions>nds|bin</extensions>
		<supports_vfs>false</supports_vfs>
		<supports_standalone>false</supports_standalone>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en_GB">Nintendo - DS (DeSmuME 2015) git</summary>
		<description lang="en_GB">Port of DeSmuME to libretro based on DeSmuME SVN circa 2015.

Supported files: .nds, .bin</description>
		<license>GPLv2</license>
		<platform>@PLATFORM@</platform>
		<source>https://github.com/kodi-game/game.libretro.desmume2015</source>
	</extension>
</addon>
